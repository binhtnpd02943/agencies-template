		"@mui/icons-material": "^5.11.11",
		"@mui/material": "^5.11.14",
		"@react-spring/web": "^10.0.1",
		"@reduxjs/toolkit": "^1.9.3",
		"antd": "^5.25.2",
		"antd-style": "^3.7.1",
		"apexcharts": "^3.37.2",
		"autoprefixer": "^10.4.21",
		"formik": "^2.2.9",
		"moment": "^2.30.1",
		"notistack": "^3.0.1",
		"postcss": "^8.5.3",
