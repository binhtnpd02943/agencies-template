{
	"name": "slim-mui-template",
	"private": false,
	"version": "1.0.0",
	"homepage": "https://themewagon.github.io/slim-free-react-mui-template",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:host": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
